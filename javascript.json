	"Comment Big": {
		"prefix": "comment-big",
		"body": [
			"/********************************************************",
			"**  COMMENT",
			"********************************************************/"
		],
		"description": "Custom print-r for logged in users"
	},

	"jQuery Wrap": {
		"prefix": "jquery-wrap",
		"body": [
			"jQuery(function($){",
			" ",
			"});"
		],
		"description": "Log output to console"
	},

	"jQuery On Load": {
		"prefix": "jquery-on-load",
		"body": [
			"jQuery(document).ready(function($) {",
			" ",
			"});"
		],
		"description": "Log output to console"
	},

	"Conole Log": {
		"prefix": "conole-log",
		"body": [
			"console.log('newVal' + newVal);"
		],
		"description": "Log output to console"
	},

	"Slick Slider": {
		"prefix": "slick-slider",
		"body": [
			"$('#jobsSlider').slick({",
			"dots: false,",
			"arrows: true,",
			"slidesToShow: 3,",
			"slidesToScroll: 1,",
			"prevArrow: IMG_URL_HERE,",
			"nextArrow: IMG_URL_HERE,",
			"});"
		],
		"description": "Log output to console"
	}
}